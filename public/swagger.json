      "post": {
        "tags": [
          "Item-Delivery"
        ],
        "description": "Item we want to deliver",
        "parameters": [
          {
            "name": "user",
            "in": "body",
            "description": "Item we want to deliver",
            "schema": {
              "$ref": "#/definitions/User"
            }
          }
        ],
        "produces": [
          "application/json"
        ],
